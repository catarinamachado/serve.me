<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="utilizador.Cliente" table="Cliente" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="informacao_pessoal" column="Informacao_pessoalID" class="utilizador.Informacao_pessoal" cascade="save-update,lock,delete" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
